<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>fallin</Authors>
    <Company />
    <Version>0.1.1</Version>
    <Copyright>Copyright Â© 2019, Brant Fallin</Copyright>
    <RepositoryUrl>https://github.com/fallin/gexf-dotnet</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Description>A .NET/C# library to generate GEXF (Graph Exchange XML Format) files</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile></PackageLicenseFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>gexf-dotnet</PackageId>
    <Product>gexf-dotnet</Product>
    <PackageTags>gexf gephi</PackageTags>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/fallin/gexf-dotnet</PackageProjectUrl>
    <PackageReleaseNotes />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.7.0" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
  </ItemGroup>

</Project>
